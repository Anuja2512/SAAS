<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ViewReportsDoctor"
        android:padding="20dp"
        android:background="@drawable/back" android:id="@+id/layout">

    <ListView
            android:layout_width="wrap_content"
            android:layout_height="455dp"
            android:id="@+id/myListView"
            android:padding="10dp"
            android:layout_marginBottom="123dp"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="89dp"
            android:layout_alignParentStart="true"
            android:layout_marginStart="80dp"
            android:layout_alignParentBottom="true" android:cacheColorHint="#2196F3">

    </ListView>

    <TextView
            android:text="All Reports"
            android:layout_width="128dp"
            android:layout_height="68dp"
            android:id="@+id/textView20"
            android:layout_above="@+id/myListView"
            android:textSize="30sp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true" android:layout_alignParentTop="true" android:gravity="center"/>


</RelativeLayout>